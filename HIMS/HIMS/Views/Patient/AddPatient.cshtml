    </form>
    </div>
    <div>
        <b>@Model.patientName </b>
        <br />
        <b>@Model.age</b>
        <br />
        <b>@Model.phone</b>
        <br />
        @* <b>@Model.address</b> *@
        <br />
    </div>
    <a href="/Home/Index">HomePage</a>
   
    
</body>
</html>
