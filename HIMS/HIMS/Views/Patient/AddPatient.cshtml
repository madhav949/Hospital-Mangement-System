
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddPatient</title>
</head>
<body>
    <div>
        <form action="/Patient/SubmitPatient" method="post">
      
    Patient Name:-
    <input type="text" name="patientName" /></br></br>
